<xs:schema xmlns:css="http://www.safelayer.com/TWS" xmlns:dss="http://www.docs.oasis-open.org/dss/2004/06/oasis-dss-1.0-core-schema-wd-27.xsd" xmlns:saml="urn:oasis:names:tc:SAML:1.0:assertion" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xades="http://uri.etsi.org/01903/v1.2.2#" xmlns:ds="http://www.w3.org/2000/09/xmldsig#" xmlns:xenc="http://www.w3.org/2001/04/xmlenc#" targetNamespace="http://www.safelayer.com/TWS" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="http://uri.etsi.org/01903/v1.2.2#" schemaLocation="XAdES.xsd"/>
	<xs:import namespace="http://www.w3.org/2001/04/xmlenc#" schemaLocation="xenc-schema.xsd"/>
	<xs:import namespace="http://www.docs.oasis-open.org/dss/2004/06/oasis-dss-1.0-core-schema-wd-27.xsd" schemaLocation="oasis-dss-1.0-core-schema-wd-27.xsd"/>
	<xs:import namespace="http://www.w3.org/2000/09/xmldsig#" schemaLocation="xmldsig-core-schema.xsd"/>
	<xs:import namespace="urn:oasis:names:tc:SAML:1.0:assertion" schemaLocation="oasis-sstc-saml-schema-assertion-1.1.xsd"/>
	<!--Common Protocol Structures-->
	<xs:annotation>
		<xs:documentation>Document identifier: TrustedX XML Schema</xs:documentation>
	</xs:annotation>
	<xs:complexType name="base64Binary">
		<xs:simpleContent>
			<xs:extension base="xs:base64Binary">
				<xs:attribute name="MimeType" type="xs:string" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:element name="Result">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="ResultMajor" type="xs:anyURI"/>
				<xs:element name="ResultMinor" type="xs:anyURI" minOccurs="0"/>
				<xs:element name="ResultMessage" type="dss:InternationalStringType" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="NameList">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Name" type="saml:NameIdentifierType" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CertRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="css:NameList" minOccurs="0"/>
				<xs:element name="EncodedValues">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="EncapsulatedX509Certificate" type="css:base64Binary" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="KeyUsage" type="xs:string" minOccurs="0">
					<!-- improved compatibility -->
					<!--					
					<xs:simpleType>
						<xs:list itemType="css:keyUsageType"/>
					</xs:simpleType>
          -->
				</xs:element>
				<xs:element ref="css:AddCertificateValues" minOccurs="0">
					<xs:annotation>
						<xs:appinfo>If CertificateTemplate is not included in the request, PssEnumResponse returns a list of selected certificates identified by its ID. If certificate information is required, include CertificateTemplate element in the request. Returned certificate fields can be explicitly defined by means of this element. </xs:appinfo>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CertResponse">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="css:NameList"/>
				<xs:element ref="css:CertificateValues"/>
				<xs:element ref="css:Result"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="KeySelector">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Name" type="saml:NameIdentifierType" minOccurs="0"/>
				<xs:element name="KeyUsage" type="xs:string" minOccurs="0">
					<!-- improved compatibility -->
					<!--					
					<xs:simpleType>
						<xs:list itemType="css:keyUsageType"/>
					</xs:simpleType>
          -->
				</xs:element>
				<xs:element name="ExtKeyUsage" type="xs:string" minOccurs="0">
					<!-- improved compatibility -->
					<!--					
					<xs:simpleType>
						<xs:list itemType="css:extKeyUsageType"/>
					</xs:simpleType>
          -->
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="SignatureDigestAlgorithmType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="md5"/>
			<xs:enumeration value="sha1"/>
			<xs:enumeration value="id_sha256"/>
			<xs:enumeration value="id_sha384"/>
			<xs:enumeration value="id_sha512"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="signatureAlgorithmType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="pkcs1-sha1WithRsaSignature"/>
			<xs:enumeration value="pkcs1-sha256WithRSAEncryption"/>
			<xs:enumeration value="pkcs1-sha384WithRSAEncryption"/>
			<xs:enumeration value="pkcs1-sha512WithRSAEncryption"/>
			<xs:enumeration value="pkcs1-md5WithRSAEncryption"/>
			<xs:enumeration value="pkcs1-md2WithRSAEncryption"/>
			<xs:enumeration value="id-dsa-with-sha1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SignatureAlgorithmUrnType">
		<xs:restriction base="xs:anyURI">
			<xs:enumeration value="urn:rsasdi-com:rsa-sha1"/>
			<xs:enumeration value="urn:rsasdi-com:rsa-sha256"/>
			<xs:enumeration value="urn:rsasdi-com:rsa-sha384"/>
			<xs:enumeration value="urn:rsasdi-com:rsa-sha512"/>
			<xs:enumeration value="urn:rsasdi-com:rsa-md2"/>
			<xs:enumeration value="urn:rsasdi-com:rsa-md5"/>
			<xs:enumeration value="urn:nist-gov:dsa"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="SignatureDigestAlgorithm" type="css:SignatureDigestAlgorithmType"/>
	<xs:simpleType name="keyUsageType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="digitalSignature"/>
			<xs:enumeration value="nonRepudiation"/>
			<xs:enumeration value="keyEncipherment"/>
			<xs:enumeration value="dataEncipherment"/>
			<xs:enumeration value="keyAgreement"/>
			<xs:enumeration value="keyCertSign"/>
			<xs:enumeration value="cRLSign"/>
			<xs:enumeration value="encipherOnly"/>
			<xs:enumeration value="decipherOnly"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="extKeyUsageType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="id-kp-serverAuth"/>
			<xs:enumeration value="id-kp-clientAuth"/>
			<xs:enumeration value="id-kp-codeSigning"/>
			<xs:enumeration value="id-kp-emailProtection"/>
			<xs:enumeration value="id-kp-profileKeyEncryption"/>
			<xs:enumeration value="id-kp-timeStamping"/>
			<xs:enumeration value="id-kp-OCSPSigning"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="MultiSignatureResult">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="SignatureResult" type="css:SignatureResultType" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="SignatureResultType">
		<xs:sequence>
			<xs:element ref="dss:Result"/>
			<xs:element ref="dss:OptionalOutputs"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="AddSignatureForm" nillable="true"/>
	<xs:element name="NewSignatureForm" type="css:SignatureFormType"/>
	<xs:element name="SignatureForm" type="css:SignatureFormType"/>
	<xs:simpleType name="SignatureFormType">
		<xs:restriction base="xs:anyURI">
			<xs:enumeration value="urn:oasis:names:tc:dss:1.0:profiles:XAdES:forms:BES"/>
			<xs:enumeration value="urn:oasis:names:tc:dss:1.0:profiles:XAdES:forms:ES-T"/>
			<xs:enumeration value="urn:oasis:names:tc:dss:1.0:profiles:XAdES:forms:ES-C"/>
			<xs:enumeration value="urn:oasis:names:tc:dss:1.0:profiles:XAdES:forms:ES-A"/>
			<xs:enumeration value="urn:safelayer:tws:dss:1.0:profiles:NoAdES"/>
			<xs:enumeration value="urn:oasis:names:tc:dss:1.0:profiles:XAdES:forms:EPES"/>
			<xs:enumeration value="urn:oasis:names:tc:dss:1.0:profiles:XAdES:forms:ES-XL"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="AddBaselineLevel" nillable="true"/>
	<xs:element name="BaselineLevel">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="css:BaselineLevelType">
					<xs:attribute name="isEN319" type="xs:boolean" use="required"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="BaselineLevelType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="None"/>
			<xs:enumeration value="B"/>
			<xs:enumeration value="T"/>
			<xs:enumeration value="LT"/>
			<xs:enumeration value="LTA"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="AddSignatureKeyInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="css:KeyInfoType" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="sign" type="xs:boolean" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="KeyInfoType">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="X509IssuerSerial"/>
				<xs:enumeration value="X509Certificate"/>
				<xs:enumeration value="X509SubjectName"/>
				<xs:enumeration value="SecurityTokenReference"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="NodeToSign">
		<xs:complexType>
			<xs:attribute name="XPath" type="xs:string"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="MultiNodeToSign">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="css:NodeToSign" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="ExtendedSignaturePlacement">
		<xs:complexType>
			<xs:choice>
				<xs:element name="XPathBefore" type="xs:string"/>
				<xs:element name="XPathAfter" type="xs:string"/>
				<xs:element name="XPathFirstChildOf" type="xs:string"/>
			</xs:choice>
			<xs:attribute name="WhichDocument" type="xs:IDREF"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="CanonicalizationMethod">
		<xs:simpleType>
			<xs:restriction base="xs:anyURI">
				<xs:enumeration value="http://www.w3.org/TR/2001/REC-xml-c14n-20010315"/>
				<xs:enumeration value="http://www.w3.org/TR/2001/REC-xml-c14n-20010315#WithComments"/>
				<xs:enumeration value="http://www.w3.org/2001/10/xml-exc-c14n#"/>
				<xs:enumeration value="http://www.w3.org/2001/10/xml-exc-c14n#WithComments"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="AddCertificateValues" type="css:AddCertificateValuesType" nillable="true"/>
	<xs:complexType name="AddCertificateValuesType">
		<xs:sequence>
			<xs:element ref="css:certificateTemplate" minOccurs="0"/>
			<xs:any namespace="##other" processContents="lax" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="binary" type="xs:boolean"/>
	</xs:complexType>
	<xs:element name="certificateTemplate" type="css:certificateType" nillable="true"/>
	<xs:element name="AddChainCertificateValues" type="css:AddCertificateValuesType" nillable="true"/>
	<xs:element name="AddNewTimeStampValues" nillable="true"/>
	<xs:element name="AddTimeStampValues" nillable="true">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="css:AddCertificateValues" minOccurs="0"/>
				<xs:element ref="css:TstInfoTemplate" minOccurs="0"/>
				<xs:element ref="css:AddRevocationValues" minOccurs="0"/>
				<xs:element ref="css:AddChainCertificateValues" minOccurs="0"/>
				<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="2"/>
			</xs:sequence>
			<xs:attribute name="binary" type="xs:boolean"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="TstInfoTemplate" nillable="true">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="SerialNumber" minOccurs="0"/>
				<xs:element name="CreationTime" minOccurs="0"/>
				<xs:element name="Policy" minOccurs="0"/>
				<xs:element name="ErrorBound" minOccurs="0"/>
				<xs:element name="Ordered" minOccurs="0"/>
				<xs:element name="TSA" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="AddRevocationValues" type="css:AddRevocationValuesType" nillable="true"/>
	<xs:element name="AddChainRevocationValues" type="css:AddRevocationValuesType" nillable="true"/>
	<xs:complexType name="AddRevocationValuesType">
		<xs:sequence>
			<xs:element ref="css:crlTemplate" minOccurs="0"/>
			<xs:element ref="css:OCSPResponseTemplate" minOccurs="0"/>
			<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="2"/>
		</xs:sequence>
		<xs:attribute name="binary" type="xs:boolean"/>
	</xs:complexType>
	<xs:element name="AddAdditionalInfoValues" nillable="true"/>
	<xs:element name="ArchivedRevocationValues" type="xades:RevocationValuesType"/>
	<xs:element name="ChainInformationValues">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="css:SingleCertInfo" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="SingleCertInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="css:CertificateValues" minOccurs="0"/>
				<xs:element ref="xades:RevocationValues" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="authorityKeyIdentifierType">
		<xs:sequence>
			<xs:element name="issuer" type="xs:string"/>
			<xs:element name="serialNumber" type="xs:base64Binary"/>
			<xs:element name="keyIdentifier" type="xs:hexBinary"/>
		</xs:sequence>
		<xs:attribute name="binary" type="xs:boolean"/>
	</xs:complexType>
	<xs:complexType name="certExtensionsType">
		<xs:sequence minOccurs="0">
			<xs:element name="extension" type="css:certExtensionType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- improved compatibility -->
	<!--
	<xs:group name="cRLDistributionPointGroup">
		<xs:sequence>
			<xs:element name="distributionPoint" type="xs:anyURI"/>
			<xs:element name="reasons" type="xs:string" minOccurs="0"/>
			<xs:element name="cRLIssuer" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	-->
	<xs:complexType name="cRLDistributionPointsType">
		<xs:sequence>
			<xs:any namespace="##targetNamespace" processContents="lax" maxOccurs="unbounded"/>
			<!-- improved compatibility -->
			<!--
			<xs:group ref="css:cRLDistributionPointGroup" maxOccurs="unbounded"/>
			-->
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="validityType">
		<xs:sequence>
			<xs:element name="notBefore" type="xs:dateTime"/>
			<xs:element name="notAfter" type="xs:dateTime"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="certificateType">
		<xs:sequence>
			<xs:element name="tbsCertificate" minOccurs="0">
				<xs:complexType>
					<xs:all>
						<xs:element name="subject" type="xs:string" minOccurs="0"/>
						<xs:element name="version" type="xs:integer" minOccurs="0"/>
						<xs:element name="issuer" type="xs:string" minOccurs="0"/>
						<xs:element name="serialNumber" type="xs:base64Binary" minOccurs="0"/>
						<xs:element name="validity" type="css:validityType" minOccurs="0"/>
						<xs:element name="extensions" type="css:certExtensionsType" minOccurs="0"/>
						<xs:element name="subjectPublicKeyInfo" minOccurs="0">
							<xs:complexType>
								<xs:all>
									<xs:element name="subjectPublicKey" type="ds:KeyValueType" minOccurs="0"/>
								</xs:all>
								<xs:attribute name="type" type="xs:string" use="required"/>
							</xs:complexType>
						</xs:element>
						<xs:element name="issuerUniqueIdentifier" type="xs:string" minOccurs="0"/>
						<xs:element name="subjectUniqueIdentifier" type="xs:string" minOccurs="0"/>
						<xs:element name="signatureAlgorithm" type="css:signatureAlgorithmType" minOccurs="0"/>
					</xs:all>
				</xs:complexType>
			</xs:element>
			<xs:element name="signatureAlgorithm" type="css:signatureAlgorithmType" minOccurs="0"/>
			<xs:element name="signature" type="xs:base64Binary" minOccurs="0"/>
			<xs:any namespace="##other" processContents="lax" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="ThumbId" type="xs:base64Binary" use="optional">
			<xs:annotation>
				<xs:appinfo>Unique identifier of the certificate. It corresponds with the base64 encoding of the SHA1 hash of the SubjectPublicKey value in the x509 certificate. This value is the same used by ds:KeyInfo elements in the DSS Signing protocol.</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="CertificateValuesType">
		<xs:all>
			<xs:element name="EncodedValues" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="EncapsulatedX509Certificate" type="css:base64Binary" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="CertificateList" type="css:certificateListType"/>
		</xs:all>
	</xs:complexType>
	<xs:element name="CertificateValues" type="css:CertificateValuesType"/>
	<xs:element name="certificate" type="css:certificateType"/>
	<xs:complexType name="certificateListType">
		<xs:sequence minOccurs="0">
			<xs:element ref="css:certificate" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="serialNumberType">
		<xs:restriction base="xs:base64Binary"/>
	</xs:simpleType>
	<xs:element name="crlTemplate" type="css:crlType" nillable="true"/>
	<xs:complexType name="crlType">
		<xs:sequence>
			<xs:element name="tbsCrl" minOccurs="0">
				<xs:complexType>
					<xs:all>
						<xs:element name="version" type="xs:integer" minOccurs="0"/>
						<xs:element name="signatureAlgorithm" type="css:signatureAlgorithmType" minOccurs="0"/>
						<xs:element name="issuer" type="xs:string" minOccurs="0"/>
						<xs:element name="thisUpdate" type="xs:dateTime" minOccurs="0"/>
						<xs:element name="nextUpdate" type="xs:dateTime" minOccurs="0"/>
						<xs:element name="revokedCertificates" type="css:revokedCertificatesType" minOccurs="0"/>
						<xs:element name="crlExtensions" type="css:crlExtensionsType" minOccurs="0"/>
						<xs:element name="issuerUniqueIdentifier" type="xs:string" minOccurs="0"/>
						<xs:element name="subjectUniqueIdentifier" type="xs:string" minOccurs="0"/>
						<xs:element name="extensions" type="css:crlExtensionsType" minOccurs="0"/>
					</xs:all>
				</xs:complexType>
			</xs:element>
			<xs:element name="signatureAlgorithm" type="css:signatureAlgorithmType" minOccurs="0"/>
			<xs:element name="signature" type="xs:hexBinary" minOccurs="0"/>
			<xs:any namespace="##other" processContents="lax" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="ThumbId" type="xs:ID">
			<xs:annotation>
				<xs:appinfo>Unique identifier of the crl.</xs:appinfo>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="crl" type="css:crlType"/>
	<xs:complexType name="revokedCertificatesType">
		<xs:sequence>
			<xs:element name="revokedCertificate" type="css:revokedCertificateType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="crlEntryExtensionsType">
		<xs:sequence>
			<xs:element name="reason" type="css:reasonCodeType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="invalidityDate" type="xs:dateTime" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="certificateIssuer" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
			<xs:any namespace="##local"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="reasonCodeType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="unspecified"/>
			<xs:enumeration value="keyCompromise"/>
			<xs:enumeration value="cACompromise"/>
			<xs:enumeration value="affiliationChanged"/>
			<xs:enumeration value="superseded"/>
			<xs:enumeration value="cessationOfOperation"/>
			<xs:enumeration value="certificateHold"/>
			<xs:enumeration value="removeFromCRL"/>
			<xs:enumeration value="privilegeWithdrawn"/>
			<xs:enumeration value="aACompromise"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="crlExtensionsType">
		<xs:sequence>
			<xs:element name="extension" type="css:crlExtensionType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="crlExtensionType">
		<xs:choice>
			<xs:element name="issuingDistributionPoint" type="xs:string" minOccurs="0"/>
			<xs:element name="cRLNumber" type="xs:hexBinary" minOccurs="0"/>
			<xs:element name="authorityKeyIdentifier" type="css:authorityKeyIdentifierType" minOccurs="0"/>
			<xs:element name="issuerAltName" type="xs:string" minOccurs="0"/>
			<xs:element name="deltaCRLIndicator" type="css:cRLDistributionPointsType" minOccurs="0"/>
			<xs:any namespace="##local"/>
		</xs:choice>
		<xs:attribute name="critical" type="xs:boolean"/>
	</xs:complexType>
	<xs:complexType name="certExtensionType">
		<xs:choice>
			<xs:element name="keyUsage" type="xs:string" minOccurs="0">
				<!--				
					<xs:simpleType>
						<xs:list itemType="css:keyUsageType"/>
					</xs:simpleType>
-->
			</xs:element>
			<xs:element name="extKeyUsage" type="xs:string" minOccurs="0">
				<!--					
					<xs:simpleType>
						<xs:list itemType="css:extKeyUsageType"/>
					</xs:simpleType>
-->
			</xs:element>
			<xs:element name="subjectAltName" type="css:subjectAltNameType" minOccurs="0"/>
			<xs:element name="issuerAltName" type="xs:string" minOccurs="0"/>
			<xs:element name="basicConstraints" type="css:basicConstraintsType" minOccurs="0"/>
